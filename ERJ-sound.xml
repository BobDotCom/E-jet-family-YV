<PropertyList>

<fx>
    <engine>
        <name>left engine</name>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/jet.wav</path>
        <mode>looped</mode>
        <condition>
            <not>
                <property>controls/engines/engine[0]/cutoff</property>
            </not>
        </condition>
        <volume>
            <property>engines/engine[0]/thrust-lb</property>
            <factor>0.00004</factor>
            <min>0.0</min>
            <max>1</max>
        </volume>
        <volume>
            <property>sim/sound/Ovolume</property>
        </volume>
        <position>
            <x>0</x>
            <y>-9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>200.0</reference-dist>
        <max-dist>500.0</max-dist>
    </engine>

        <engine>
