    <animation>
        <name>BacklightMainMaterial</name>
        <type>material</type>
        <object-name>BacklitObjectsMain</object-name>
        <transparency><alpha>0.1</alpha></transparency>
    </animation>
    
    <animation>
        <name>BacklitObjectsPedestal</name>
        <object-name>pedestal</object-name>
        <object-name>pedestal.001</object-name>
        <object-name>pedestal.002</object-name>
        <object-name>pedestal.005</object-name>
        <object-name>pedestal.008</object-name>
        <object-name>box.004</object-name>
        <object-name>rat-deploy</object-name>
    </animation>
    
    <animation>
        <name>BacklightPedestal</name>
        <type>material</type>
        <object-name>BacklitObjectsPedestal</object-name>
        <emission>
            <red>1.0</red>
            <green>0.75</green>
            <blue>0.5</blue>
            <factor-prop>/systems/electrical/outputs/pedestal-light</factor-prop>
        </emission>
    </animation>
    
    <animation>
        <name>BacklightEngineControl</name>
        <type>material</type>
        <object-name>engines.plate</object-name>
        <object-name>e1.case</object-name>
        <object-name>e2.case</object-name>
        <emission>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
        </emission>
    </animation>
    
    <!-- master warning/caution buttons -->
    
    <animation>
        <type>material</type>
        <object-name>MasterWarningBtn.left</object-name>
        
        <condition>
            <and>
                <property>/instrumentation/eicas/master/warning</property>
                <greater-than>
                    <property>systems/electrical/outputs/master-warn-caution[0]</property>
                    <value>15</value>
                </greater-than>
            </and>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
        </emission>
    </animation>
    
    <animation>
        <type>material</type>
        <object-name>MasterCautionBtn.left</object-name>
        <condition>
            <and>
                <property>/instrumentation/eicas/master/caution</property>
                <greater-than>
                    <property>systems/electrical/outputs/master-warn-caution[0]</property>
                    <value>15</value>
                </greater-than>
            </and>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
        </emission>
    </animation>
    
    <animation>
        <type>material</type>
        <object-name>MasterWarningBtn.right</object-name>
        
        <condition>
            <and>
                <property>/instrumentation/eicas/master/warning</property>
                <greater-than>
                    <property>systems/electrical/outputs/master-warn-caution[1]</property>
                    <value>15</value>
                </greater-than>
            </and>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
        </emission>
    </animation>
    
    <animation>
        <type>material</type>
        <object-name>MasterCautionBtn.right</object-name>
        <condition>
            <and>
                <property>/instrumentation/eicas/master/caution</property>
                <greater-than>
                    <property>systems/electrical/outputs/master-warn-caution[1]</property>
                    <value>15</value>
                </greater-than>
            </and>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
        </emission>
    </animation>
    
    <animation>
        <type>material</type>
        <object-name>MasterWarningBtn.left</object-name>
        <object-name>MasterCautionBtn.left</object-name>
        <object-name>MasterWarningBtn.right</object-name>
        <object-name>MasterCautionBtn.right</object-name>
        
        <ambient>
            <red>0.2</red>
            <green>0.2</green>
            <blue>0.2</blue>
        </ambient>
        <diffuse>
            <red>0.2</red>
            <green>0.2</green>
            <blue>0.2</blue>
        </diffuse>
    </animation>

