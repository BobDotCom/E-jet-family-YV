<?xml version = '1.0' encoding = 'UTF-8' ?>
<PropertyList>
  
  <path>../../Models/Primus-Epic/primus_epic_eicas.ac</path>

	<!-- Power on/off -->
	<animation>
		<name>Power on/off</name>
		<type>select</type>
		<object-name>off</object-name>
		<condition>
			<less-than>
				<property>systems/electrical/outputs/eicas</property>
				<value>15</value>
			</less-than>
		</condition>
	</animation>

</PropertyList>
