<PropertyList>
    <filter>
        <name>Chrono minutes</name> 
        <type>gain</type>
        <gain>1.0</gain> 
        <input>
            <expression>
                <floor>
                    <div>
                        <property>instrumentation/chrono/elapsed_time/total</property>
                        <value>60</value>
                    </div>
                </floor>
            </expression>
        </input>
        <output>instrumentation/chrono/elapsed_time/minute</output>
    </filter>
</PropertyList>
