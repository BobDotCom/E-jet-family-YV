<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
    <!--Altitude indication-->
    <filter>
        <name>Altitude 10000</name> 
        <type>gain</type>
        <gain>1.0</gain> 
        <input>
            <expression>
                <floor>
                    <div>
                        <property>instrumentation/altimeter[2]/indicated-altitude-ft</property>
                        <value>10000</value>
                    </div>
                </floor>
            </expression>
        </input>
        <output>instrumentation/iess/alt-10000</output>
    </filter>
    <filter>
        <name>Altitude 1000</name> 
        <type>gain</type>
        <gain>1.0</gain> 
        <input>
            <expression>
                <floor>
                    <div>
                        <property>instrumentation/altimeter[2]/indicated-altitude-ft</property>
                        <value>1000</value>
                    </div>
                </floor>
            </expression>
        </input>
        <output>instrumentation/iess/alt-1000</output>
    </filter>
    <filter>
        <name>Altitude 100</name> 
        <type>gain</type>
        <gain>1.0</gain> 
        <input>
            <expression>
                <floor>
                    <div>
                        <property>instrumentation/altimeter[2]/indicated-altitude-ft</property>
                        <value>100</value>
                    </div>
                </floor>
            </expression>
        </input>
        <output>instrumentation/iess/alt-100</output>
    </filter>
    <filter>
        <name>Altitude 1</name> 
        <type>gain</type>
        <gain>1.0</gain> 
        <input>
            <expression>
                <floor>
                    <property>instrumentation/altimeter[2]/indicated-altitude-ft</property>
                </floor>
            </expression>
        </input>
        <output>instrumentation/iess/alt-1</output>
    </filter>
    
    
    <filter>
        <name>Airspeed 10</name> 
        <type>gain</type>
        <gain>1.0</gain> 
        <input>
            <expression>
                <floor>
                    <div>
                        <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
                        <value>10</value>
                    </div>
                </floor>
            </expression>
        </input>
        <output>instrumentation/iess/asi-10</output>
    </filter>
    
    <filter>
        <name>Airspeed 100</name> 
        <type>gain</type>
        <gain>1.0</gain> 
        <input>
            <expression>
                <floor>
                    <div>
                        <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
                        <value>100</value>
                    </div>
                </floor>
            </expression>
        </input>
        <output>instrumentation/iess/asi-100</output>
    </filter>
    
    <filter>
        <name>VSneedle</name>
        <type>gain</type>
        <gain>1.0</gain>
        <input>
            <expression>
                <table>
                    <prod>
                        <property>/velocities/vertical-speed-fps</property> 
                        <value>60</value>
                    </prod>
                    <entry>
                        <ind>-4000</ind><dep>-56.36</dep>
                    </entry>
                    <entry>
                        <ind>-2000</ind><dep>-49.31</dep>
                    </entry>
                    <entry>
                        <ind>-1000</ind><dep>-40.09</dep>
                    </entry>
                    <entry>
                        <ind>0</ind><dep>0</dep>
                    </entry>
                    <entry>
                        <ind>1000</ind><dep>40.09</dep>
                    </entry>
                    <entry>
                        <ind>2000</ind><dep>49.31</dep>
                    </entry>
                    <entry>
                        <ind>4000</ind><dep>56.36</dep>
                    </entry>
                </table>
            </expression>
        </input>
        <output>instrumentation/iess/vs-needle</output>
    </filter>
    
    <filter>
        <name>VMO</name>
        <type>gain</type>
        <gain>1.0</gain>
        <input>
            <condition>
                <equals>
                    <property>/controls/flight/flaps</property>
                    <value>0</value>
                </equals>
            </condition>
            <expression>
                <table>
                    <property>/position/altitude-ft</property> 
                    <entry>
                        <ind>0</ind><dep>300</dep>
                    </entry>
                    <entry>
                        <ind>8000</ind><dep>300</dep>
                    </entry>
                    <entry>
                        <ind>10000</ind><dep>320</dep>
                    </entry>
                    <entry>
                        <ind>29000</ind><dep>320</dep>
                    </entry>
                    <entry>
                        <ind>41000</ind><dep>244</dep>
                    </entry>
                </table>
            </expression>
        </input>
        <input>
            <condition>
                <greater-than>
                    <property>/controls/flight/flaps</property>
                    <value>0.0</value>
                </greater-than>
            </condition>
            <expression>
                <table>
                    <property>/controls/flight/flaps</property> 
                    <entry>
                        <ind>0.1</ind><dep>230</dep>
                    </entry>
                    <entry>
                        <ind>0.2</ind><dep>215</dep>
                    </entry>
                    <entry>
                        <ind>0.3</ind><dep>200</dep>
                    </entry>
                    <entry>
                        <ind>0.4</ind><dep>180</dep>
                    </entry>
                    <entry>
                        <ind>0.5</ind><dep>180</dep>
                    </entry>
                    <entry>
                        <ind>0.6</ind><dep>165</dep>
                    </entry>
                </table>
            </expression>
        </input>
        <output>instrumentation/iess/vmo</output>
    </filter>
</PropertyList>
