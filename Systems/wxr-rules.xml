<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

    <filter>
        <name>WXR mode</name>
        <type>gain</type>
        <gain>1</gain>
        <input>
            <expression>
                <max>
                    <property>/instrumentation/mfd[0]/wx-mode</property>
                    <property>/instrumentation/mfd[1]/wx-mode</property>
                </max>
            </expression>
        </input>
        <output>/instrumentation/wxr/mode</output>
    </filter>

</PropertyList>
