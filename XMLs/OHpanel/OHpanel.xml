<!-- Windshield Wiper Controls -->

	<animation>
		<name>Wiper Pick Left</name>
		<type>pick</type>
		<object-name>wiper.left</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-cycle</command>
				<property>controls/switches/wiper-left</property>
				<value>0</value>
				<value>1</value>
				<value>2</value>
				<value>3</value>
			</binding>
		</action>
	</animation>

	<animation>
		<name>Wiper Pick Right</name>
		<type>pick</type>
		<object-name>wiper.right</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-cycle</command>
				<property>controls/switches/wiper-right</property>
				<value>0</value>
				<value>1</value>
				<value>2</value>
				<value>3</value>
			</binding>
		</action>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>wiper.left</object-name>
		<property>controls/switches/wiper-left</property>
		<interpolation>
			<entry><ind>0</ind><dep>60</dep></entry>
			<entry><ind>1</ind><dep>0</dep></entry>
			<entry><ind>2</ind><dep>-60</dep></entry>
			<entry><ind>3</ind><dep>-120</dep></entry>
		</interpolation>
		<axis>
			<x1-m>-12.1821</x1-m>
			<y1-m>-0.0413</y1-m>
			<z1-m>1.7225</z1-m>
			<x2-m>-12.1673</x2-m>
			<y2-m>-0.0413</y2-m>
			<z2-m>1.6959</z2-m>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>wiper.right</object-name>
		<property>controls/switches/wiper-right</property>
		<interpolation>
			<entry><ind>0</ind><dep>60</dep></entry>
			<entry><ind>1</ind><dep>0</dep></entry>
			<entry><ind>2</ind><dep>-60</dep></entry>
			<entry><ind>3</ind><dep>-120</dep></entry>
		</interpolation>
		<axis>
			<x1-m>-12.1821</x1-m>
			<y1-m>0.0378</y1-m>
			<z1-m>1.7225</z1-m>
			<x2-m>-12.1673</x2-m>
			<y2-m>0.0378</y2-m>
			<z2-m>1.6959</z2-m>
		</axis>
	</animation>

<!-- Fire Controls -->

	<animation>
		<name>EXTG Left</name>
		<type>pick</type>
		<object-name>extg.left</object-name>
		<object-name>extg.left.light</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/engines/engine/fire-bottle-discharge</property>
			</binding>
		</action>
	</animation>
	<animation>
		<name>EXTG Right</name>
		<type>pick</type>
		<object-name>extg.right</object-name>
		<object-name>extg.right.light</object-name>
		<action>
			<button>0</button>
