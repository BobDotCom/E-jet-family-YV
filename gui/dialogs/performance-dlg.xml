<?xml version="1.0"?>
<PropertyList>
    <name>performance</name>
	<layout>vbox</layout>
	<pref-width>800</pref-width>

    <group>
        <layout>hbox</layout>
		<empty>
			<stretch>1</stretch>
		</empty>

        <text>
            <label>Performance</label>
        </text>

		<empty>
			<stretch>1</stretch>
		</empty>

		<button>
			<pref-width>16</pref-width>
			<pref-height>16</pref-height>
			<legend></legend>
			<keynum>27</keynum>
			<border>2</border>
			<binding>
				<command>dialog-close</command>
			</binding>
		</button>
    </group>

    <hrule/>

    <group>
        <layout>vbox</layout>

        <text>
            <label>Speed Schedule</label>
        </text>
        <group>
            <layout>hbox</layout>
            <halign>center</halign>
            <valign>top</valign>

            <group>
                <layout>vbox</layout>
                <valign>top</valign>
                <pref-width>250</pref-width>
                <text>
                    <label>Takeoff</label>
                </text>
                <group>
                    <layout>hbox</layout>
                    <text>
                        <label>V1</label>
                        <pref-width>15</pref-width>
                    </text>
                    <input>
                        <property>/controls/flight/vspeeds/departure/v1</property>
                        <label>KIAS</label>
                        <binding>
                            <command>dialog-apply</command>
                        </binding>
                    </input>
                </group>
                <group>
                    <layout>hbox</layout>
                    <text>
                        <label>Vr</label>
                        <pref-width>15</pref-width>
                    </text>
                    <input>
                        <property>/controls/flight/vspeeds/departure/vr</property>
                        <label>KIAS</label>
                        <binding>
                            <command>dialog-apply</command>
                        </binding>
                    </input>
                </group>
                <group>
                    <layout>hbox</layout>
                    <text>
                        <label>V2</label>
                        <pref-width>15</pref-width>
                    </text>
                    <input>
                        <property>/controls/flight/vspeeds/departure/v2</property>
                        <label>KIAS</label>
                        <binding>
                            <command>dialog-apply</command>
                        </binding>
                    </input>
                </group>
                <group>
                    <layout>hbox</layout>
                    <text>
                        <label>Vfs</label>
                        <pref-width>15</pref-width>
                    </text>
                    <input>
                        <property>/controls/flight/vspeeds/departure/vfs</property>
                        <label>KIAS</label>
                        <binding>
                            <command>dialog-apply</command>
                        </binding>
                    </input>
                </group>
            </group>
            <vrule/>
            <group>
                <layout>vbox</layout>
                <valign>top</valign>
                <pref-width>250</pref-width>
                <text>
                    <label>Departure/Climb</label>
                </text>
                <group>
                    <layout>hbox</layout>
                    <text>
                        <label>Vdep</label>
                        <pref-width>15</pref-width>
                    </text>
                    <input>
                        <property>/controls/flight/speed-schedule/departure</property>
                        <label>KIAS</label>
                        <binding>
                            <command>dialog-apply</command>
                        </binding>
                    </input>
                </group>
                <group>
                    <layout>hbox</layout>
                    <text>
                        <label>Vclb&lt;10k</label>
                        <pref-width>15</pref-width>
                    </text>
                    <input>
                        <property>/controls/flight/speed-schedule/climb-below-10k</property>
                        <label>KIAS</label>
                        <binding>
                            <command>dialog-apply</command>
                        </binding>
                    </input>
                </group>
                <group>
                    <layout>hbox</layout>
                    <text>
                        <label>Vclb</label>
                        <pref-width>15</pref-width>
                    </text>
                    <input>
                        <property>/controls/flight/speed-schedule/climb-kts</property>
                        <label>KIAS</label>
                        <binding>
                            <command>dialog-apply</command>
                        </binding>
                    </input>
                </group>
                <group>
                    <layout>hbox</layout>
                    <text>
                        <label>Mclb</label>
                        <pref-width>15</pref-width>
                    </text>
                    <input>
                        <property>/controls/flight/speed-schedule/climb-mach</property>
                        <label>Mach</label>
                        <binding>
                            <command>dialog-apply</command>
                        </binding>
                    </input>
                </group>
            </group>
        </group>

        <group>
            <layout>hbox</layout>
            <halign>center</halign>
            <valign>top</valign>

            <group>
                <layout>vbox</layout>
                <valign>top</valign>
                <pref-width>250</pref-width>
                <text>
                    <label>Cruise/Descent</label>
                </text>
                <group>
                    <layout>hbox</layout>
                    <text>
                        <label>Vcruise</label>
                        <pref-width>15</pref-width>
                    </text>
                    <input>
                        <property>/controls/flight/speed-schedule/cruise-kts</property>
                        <label>KIAS</label>
                        <binding>
                            <command>dialog-apply</command>
                        </binding>
                    </input>
                </group>
                <group>
                    <layout>hbox</layout>
                    <text>
                        <label>Mcruise</label>
                        <pref-width>15</pref-width>
                    </text>
                    <input>
                        <property>/controls/flight/speed-schedule/cruise-mach</property>
                        <label>KIAS</label>
                        <binding>
                            <command>dialog-apply</command>
                        </binding>
                    </input>
                </group>
                <group>
                    <layout>hbox</layout>
                    <text>
                        <label>Vdsc&lt;10k</label>
                        <pref-width>15</pref-width>
                    </text>
                    <input>
                        <property>/controls/flight/speed-schedule/descent-below-10k</property>
                        <label>KIAS</label>
                        <binding>
                            <command>dialog-apply</command>
                        </binding>
                    </input>
                </group>
                <group>
                    <layout>hbox</layout>
                    <text>
                        <label>Vdsc</label>
                        <pref-width>15</pref-width>
                    </text>
                    <input>
                        <property>/controls/flight/speed-schedule/descent-kts</property>
                        <label>KIAS</label>
                        <binding>
                            <command>dialog-apply</command>
                        </binding>
                    </input>
                </group>
                <group>
                    <layout>hbox</layout>
                    <text>
                        <label>Mdsc</label>
                        <pref-width>15</pref-width>
                    </text>
                    <input>
                        <property>/controls/flight/speed-schedule/descent-mach</property>
                        <label>Mach</label>
                        <binding>
                            <command>dialog-apply</command>
                        </binding>
                    </input>
                </group>
            </group>
            <vrule/>
            <group>
                <layout>vbox</layout>
                <valign>top</valign>
                <pref-width>250</pref-width>
                <text>
                    <label>Approach</label>
                </text>
                <group>
                    <layout>hbox</layout>
                    <text>
                        <label>Vref</label>
                        <pref-width>15</pref-width>
                    </text>
                    <input>
                        <property>/controls/flight/vspeeds/approach/vref</property>
                        <label>KIAS</label>
                        <binding>
                            <command>dialog-apply</command>
                        </binding>
                    </input>
                </group>
                <group>
                    <layout>hbox</layout>
                    <text>
                        <label>Vinitial-appr</label>
                        <pref-width>15</pref-width>
                    </text>
                    <input>
                        <property>/controls/flight/speed-schedule/initial-approach</property>
                        <label>KIAS</label>
                        <binding>
                            <command>dialog-apply</command>
                        </binding>
                    </input>
                </group>
                <group>
                    <layout>hbox</layout>
                    <text>
                        <label>Vappr</label>
                        <pref-width>15</pref-width>
                    </text>
                    <input>
                        <property>/controls/flight/vspeeds/approach/vappr</property>
                        <label>KIAS</label>
                        <binding>
                            <command>dialog-apply</command>
                        </binding>
                    </input>
                </group>
                <group>
                    <layout>hbox</layout>
                    <text>
                        <label>Vac</label>
                        <pref-width>15</pref-width>
                    </text>
                    <input>
                        <property>/controls/flight/vspeeds/approach/vac</property>
                        <label>KIAS</label>
                        <binding>
                            <command>dialog-apply</command>
                        </binding>
                    </input>
                </group>
            </group>
        </group>
        <hrule/>
        <group>
            <layout>vbox</layout>
            <halign>center</halign>
            <valign>top</valign>
            <text>
                <label>Thrust Ratings</label>
            </text>
            <group>
                <layout>hbox</layout>
                <text>
                    <label>TO-</label>
                </text>
                <select>
                    <property>/controls/flight/trs/to</property>
                    <value>1</value>
                    <value>2</value>
                    <value>3</value>
                    <binding>
                        <command>dialog-apply</command>
                    </binding>
                </select>
            </group>
            <group>
                <layout>hbox</layout>
                <checkbox>
                    <label>Flex T/O</label>
                    <property>/controls/flight/trs/flex-to</property>
                    <halign>left</halign>
                    <binding>
                        <command>dialog-apply</command>
                    </binding>
                </checkbox>
            </group>
            <group>
                <text>
                    <label>TRS mode</label>
                </text>
                <layout>vbox</layout>
                <combo>
                    <pref-width>250</pref-width>
                    <property>/controls/flight/trs/mode</property>
                    <value>AUTO</value>
                    <value>TO</value>
                    <value>GA</value>
                    <value>CLB</value>
                    <value>CRZ</value>
                    <value>CON</value>
                    <binding>
                        <command>dialog-apply</command>
                    </binding>
                </combo>
            </group>
        </group>

    </group>

</PropertyList>

